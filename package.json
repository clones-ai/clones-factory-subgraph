{
  "name": "clones-factory-subgraph",
  "version": "1.0.0",
  "description": "The Graph subgraph for Clones Factory system",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy",
    "create-local": "graph create --node http://localhost:8020/ clones/factory",
    "remove-local": "graph remove --node http://localhost:8020/ clones/factory",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 clones/factory",
    "deploy-sepolia": "graph deploy --studio clones-factory-sepolia subgraph.yaml",
    "deploy-mainnet": "graph deploy --studio clones-factory-mainnet subgraph-mainnet.yaml",
    "prepare-sepolia": "graph codegen subgraph.yaml && graph build subgraph.yaml",
    "prepare-mainnet": "graph codegen subgraph-mainnet.yaml && graph build subgraph-mainnet.yaml",
    "test-queries": "node scripts/test-queries.js"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.97.1",
    "@graphprotocol/graph-ts": "0.35.1",
    "graphql": "^16.11.0",
    "graphql-request": "^7.2.0",
    "ipfs-http-client": "^60.0.1"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  }
}
